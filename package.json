{
  "name": "ds-boilerplate-new",
  "version": "0.1.1",
  "description": "Collabur Design System",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/cds/cds.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "concurrently \"rimraf dist/\" \"yarn pre-build\" \"node --max_old_space_size=4096 ./node_modules/@stencil/core/bin/stencil build --docs\"",
    "build:ds": "yarn build && yarn pre-build:storybook && yarn build:storybook",
    "build:storybook": "rimraf storybook/ && yarn pre-build:storybook && build-storybook -c .storybook -o storybook/ && cp -R www/assets storybook/assets",
    "build:npm": "yarn && npm run test && cross-env BUILD_TARGET=\"npm\" yarn build",
    "build:www": "yarn && rimraf www && cross-env BUILD_TARGET=\"www\" yarn build && cross-env BUILD_TARGET=\"www\" yarn build:storybook",
    "build:docs-assets": "yarn build:www && rimraf docs/static/ && make-dir docs/static && cp -R storybook docs/static/storybook && cp -R dist docs/static/dist",
    "build:docs": "yarn build:docs-assets && yarn docs:build",
    "copy:headers": "cp src/_headers storybook/_headers",
    "start": "set NODE_ENV=development&& concurrently \"yarn pre-build\" \"node --max_old_space_size=4096 ./node_modules/@stencil/core/bin/stencil build --dev --watch --recursive --serve\"",
    "watch": "set NODE_ENV=development&& concurrently \"yarn pre-build\" \"node --max_old_space_size=4096 ./node_modules/@stencil/core/bin/stencil build --watch --recursive --serve\"",
    "storybook.run": "yarn pre-build:storybook && start-storybook -p 9006 -s www",
    "storybook": "yarn pre-build:storybook && npm-run-all --parallel watch storybook.run",
    "test:lint:ts": "eslint --ext .ts,.tsx --ignore-path .gitignore src/components/ src/elements/ src/views/",
    "test:lint:scss": "stylelint 'src/**/*.scss' --syntax scss",
    "pre-build": "collabur-init && collabur-config && collabur-map && collabur-assets && node ./build-scripts/assets-path.js",
    "pre-build:storybook": "node ./build-scripts/storybook-ds.js",
    "test": "stencil test --spec",
    "test:watch": "stencil test --spec --watch",
    "test:e2e": "stencil test --e2e",
    "docs:start": "cd docs && yarn clean && yarn develop",
    "docs:build": "cd docs && yarn install && yarn clean && yarn build --prefix-paths && rimraf public/assets/assets",
    "docs:deploy": "cd docs && yarn gatsby-plugin-s3 deploy --yes",
    "collabur:update": "collabur-init && collabur-config && collabur-map && collabur-assets"
  },
  "dependencies": {
    "@creativebulma/bulma-collapsible": "^1.0.4",
    "@iconfu/svg-inject": "^1.2.3",
    "@popperjs/core": "^2.10.2",
    "@stencil/core": "^2.7.0",
    "@storybook/api": "^5.2.8",
    "animate.css": "^4.1.1",
    "bulma-calendar": "^6.1.13",
    "bulma-divider": "^0.2.0",
    "bulma-modal-fx": "^1.1.1",
    "bulma-scss": "^0.9.3",
    "bulma-switch": "^2.0.0",
    "classnames": "^2.3.1",
    "confetti-js": "0.0.18",
    "flickity": "^2.2.2",
    "hammerjs": "^2.0.8",
    "jsbarcode": "^3.11.5",
    "lodash": "^4.17.21",
    "lottie-web": "^5.7.14",
    "pulltorefreshjs": "^0.1.22",
    "smoothscroll-polyfill": "^0.4.4",
    "stencil-fragment": "^1.0.1",
    "web-component-analyzer": "^1.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@collabur/ds-scripts": "^0.1.8",
    "@fortawesome/fontawesome-pro": "^5.15.4",
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/sass": "^1.5.2",
    "@storybook/addon-a11y": "^5.2.8",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-centered": "^5.2.8",
    "@storybook/addon-docs": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-notes": "^5.2.8",
    "@storybook/addon-storysource": "^5.2.8",
    "@storybook/addon-viewport": "^5.2.8",
    "@storybook/html": "^5.2.8",
    "@storybook/theming": "^5.2.8",
    "@types/hammerjs": "^2.0.40",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.10.5",
    "@types/puppeteer": "^5.4.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.3.7",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.4.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "make-dir-cli": "^3.0.0",
    "moment": "^2.29.1",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "puppeteer": "^10.4.0",
    "react": "^17.0.2",
    "react-is": "^17.0.2",
    "replace-in-file": "^6.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.2.0",
    "storybook-addon-paddings": "^2.0.2",
    "style-loader": "^3.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.21.0",
    "ts-loader": "^6.2.1",
    "webpack": "^4.41.2"
  },
  "license": "MIT"
}
